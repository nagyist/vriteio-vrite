{
  "name": "@vrite/pages",
  "version": "0.5.0-alpha.0",
  "private": false,
  "description": "Astro toolkit for creating technical publications",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "unbuild",
    "prepublish": "pnpm run build",
    "publish": "pnpm publish --access public --no-git-checks"
  },
  "bugs": {
    "url": "https://github.com/vriteio/vrite/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/vriteio/vrite.git",
    "directory": "packages/pages"
  },
  "keywords": [
    "vrite",
    "headless cms",
    "cms",
    "typescript",
    "javascript",
    "documentation",
    "docs",
    "blog",
    "technical",
    "publication",
    "astro"
  ],
  "typesVersions": {
    "*": {
      "types": [
        "./dist/types.d.ts"
      ]
    }
  },
  "exports": {
    ".": "./dist/index.mjs"
  },
  "main": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "dependencies": {
    "@vrite/sdk": "workspace:*",
    "acorn": "^8.12.1"
  },
  "devDependencies": {
    "astro": "^4.13.1",
    "unbuild": "^2.0.0",
    "vite": "^5.3.3",
    "mdast-util-mdx": "^3.0.0",
    "vfile": "^6.0.1"
  }
}
